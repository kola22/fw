{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "noEmitOnError": true,
    "target": "es5",
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false,
    "types": [
      "node"
    ]
  },
  "exclude": [
    "node_modules",
    "typings"
  ]
} 
